@{
    Layout = "_Layout";
}

<!DOCTYPE html>

<html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>Saved</title>
    </head>
    <body>
        <div>
        @{
            var saved = System.IO.Directory.GetFiles("Saved/").ToList();
            foreach (var save in saved)
            {
                <a asp-action="Saved" asp-route-name="@save"><p>@save</p></a>
            }
        }
        </div>
   </body>
</html>